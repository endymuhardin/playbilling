CREATE TABLE BILLING_SEQUENCER(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0)  NOT NULL PRIMARY KEY,NAME VARCHAR(200),NEXT_VALUE INTEGER,CONSTRAINT SYS_CT_1 UNIQUE(NAME))
CREATE TABLE BILLING_SIMPLE_TARIFF(ID INTEGER NOT NULL PRIMARY KEY,NAME VARCHAR(200),RATE DECIMAL,CHARGE_UNIT INTEGER,TIME_UNIT VARCHAR,GRACE_PERIOD INTEGER,GRACE_PERIOD_CHARGE DECIMAL,CONSTRAINT SYS_CT_3 UNIQUE(NAME))
CREATE TABLE BILLING_MEMBER(ID INTEGER NOT NULL PRIMARY KEY,USERNAME VARCHAR,PASSWORD VARCHAR,FULLNAME VARCHAR,EMAIL VARCHAR,ADDRESS VARCHAR,TARIFF_ID INTEGER,AUTOCONNECT BOOLEAN,CREDIT DECIMAL,SESSION_EXPIRATION_LIMIT BIGINT,MEMBER_STATUS VARCHAR,MEMBER_TYPE VARCHAR,REGISTER_DATE TIMESTAMP,EXPIRE_DATE TIMESTAMP,CONSTRAINT SYS_CT_5 UNIQUE(USERNAME),CONSTRAINT SYS_FK_6 FOREIGN KEY(TARIFF_ID) REFERENCES BILLING_SIMPLE_TARIFF(ID))
CREATE TABLE BILLING_SESSION(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0)  NOT NULL PRIMARY KEY,MEMBER_ID INTEGER,IP_ADDRESS VARCHAR,START_TIME TIMESTAMP,END_TIME TIMESTAMP,LAST_VISIT TIMESTAMP,EXPIRATION_LIMIT INTEGER,CHARGE DECIMAL,SESSION_STATUS VARCHAR,CONSTRAINT SYS_FK_10 FOREIGN KEY(MEMBER_ID) REFERENCES BILLING_MEMBER(ID))
CREATE USER SA PASSWORD "" ADMIN
